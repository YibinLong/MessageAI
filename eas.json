{
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "GOOGLE_SERVICES_JSON": "sensitive"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "GOOGLE_SERVICES_JSON": "sensitive"
            },
            "prebuildCommand": "echo $GOOGLE_SERVICES_JSON > google-services.json"
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "GOOGLE_SERVICES_JSON": "sensitive"
            },
            "prebuildCommand": "echo $GOOGLE_SERVICES_JSON > google-services.json"
        }
    }
}